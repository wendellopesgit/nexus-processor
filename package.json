{
  "name": "nexus-processor",
  "version": "1.0.0",
  "description": "Solução técnica para o desafio: Node.JS - Processamento de Eventos em Tempo Real",
  "main": "index.js",
  "author": "Wendell Lopes <wls.lopes@gmail.com>",
  "license": "UNLICENSED",
  "homepage": "https://github.com/wendellopesgit/nexus-processor#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/wendellopesgit/nexus-processor.git"
  },
  "bugs": {
    "url": "https://github.com/wendellopesgit/nexus-processor/issues"
  },
  "engines": {
    "node": ">=22.0.0"
  },
  "keywords": [
    "challenge",
    "real-time",
    "event-processing"
  ],
  "scripts": {
    "lint": "eslint . --max-warnings=0 --no-warn-ignored --ext .ts,.tsx --fix",
    "build": "tsc && tsc-alias",
    "format": "prettier --write \"src/**/*.{ts,tsx,js,jsx,json}\"",
    "prepare": "husky",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:verbose": "jest --verbose"
  },
  "dependencies": {
    "amqplib": "^0.10.8",
    "bull": "^4.16.5",
    "dotenv": "^17.2.0",
    "mongoose": "^8.16.3",
    "winston": "^3.17.0"
  },
  "devDependencies": {
    "@commitlint/cli": "^19.8.1",
    "@commitlint/config-conventional": "^19.8.1",
    "@eslint/js": "^9.30.1",
    "@types/amqplib": "^0.10.7",
    "@types/jest": "^30.0.0",
    "@types/node": "^24.0.13",
    "@typescript-eslint/eslint-plugin": "^8.36.0",
    "@typescript-eslint/parser": "^8.36.0",
    "eslint": "^9.30.1",
    "eslint-config-prettier": "^10.1.5",
    "eslint-plugin-prettier": "^5.5.1",
    "globals": "^16.3.0",
    "husky": "^9.1.7",
    "jest": "^30.0.4",
    "lint-staged": "^16.1.2",
    "prettier": "^3.6.2",
    "ts-jest": "^29.4.0",
    "ts-node": "^10.9.2",
    "tsc-alias": "^1.8.16",
    "typescript": "^5.8.3"
  },
  "lint-staged": {
    "*.{ts,js}": [
      "eslint --max-warnings=0 --no-warn-ignored --ext .ts,.tsx --fix",
      "prettier --write"
    ],
    "*.{json,md,yml,yaml}": [
      "prettier --write"
    ]
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  }
}
